{
    "env": {
        "browser": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "airbnb"
    ],
    "plugins": [
        "jest",
        "react"
    ],
    "rules": {
        "indent": ["error", 4],
        "no-unused-vars": ["warn", { "args": "none" }],
        "react/jsx-indent": ["warn", 4],
        "react/jsx-indent-props": ["warn", 4],
        "max-len": ["warn", 120]
    }
}